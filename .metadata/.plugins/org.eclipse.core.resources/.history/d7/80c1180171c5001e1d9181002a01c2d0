package core;
class Laptop{
	int ram;
	int storage;
	double display;
	String model;
	Laptop(int ram,int storage,double display,String model){
		this.ram = ram;
		this.storage = storage;
		this.display = display;
		this.model = model;
	}
	public void setRam(int ram) {
		this.ram = ram;
	}
}
public class P017_Encapsulation {
	public static void main(String[] args) {
	}
}
