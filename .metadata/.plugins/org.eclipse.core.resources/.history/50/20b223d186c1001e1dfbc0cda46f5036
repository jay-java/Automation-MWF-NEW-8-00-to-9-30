package core;

class Parent{
	public void parentFunction() {
		System.out.println("parent");
	}
}
class Child extends Parent{
	public void childFunction() {
		System.out.println("Child");
	}
}
public class P014_Inheritance {
	public static void main(String[] args) {
		Parent p = new Parent();
		p.parentFunction();
	}
}
